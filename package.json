{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn",
    "bootstrap:undo": "lerna clean --yes",
    "bootstrap:upgrade": "yarn upgrade-interactive --latest",
    "clean": "lerna run --parallel clean",
    "build": "yarn workspaces run build",
    "test:unit:coverage": "yarn workspaces run test:unit:coverage",
    "check:types": "yarn workspaces run check:types",
    "lint": "eslint",
    "prepare": "yarn build",
    "publish:version": "lerna publish",
    "publish:canary": "lerna publish --canary"
  },
  "devDependencies": {
    "@emotion/jest": "^11.13.0",
    "@eslint/js": "^9.17.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3 ",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^24.1.1",
    "lerna": "^7.4.2",
    "ts-jest": "^29.2.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  },
  "license": "MIT"
}

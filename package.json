{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn",
    "bootstrap:undo": "lerna clean --yes",
    "bootstrap:upgrade": "yarn upgrade-interactive --latest",
    "clean": "lerna run --parallel clean",
    "build": "yarn workspaces run build",
    "test:unit:coverage": "yarn workspaces run test:unit:coverage",
    "check:types": "yarn workspaces run check:types",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "yarn build",
    "publish:version": "lerna publish",
    "publish:canary": "lerna publish --canary"
  },
  "devDependencies": {
    "@emotion/jest": "^11.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3 ",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^22.1.0",
    "lerna": "^7.1.4",
    "ts-jest": "^29.1.0"
  },
  "resolutions": {
    "css-loader": "^5.0.0",
    "dotenv-webpack": "^6.0.0",
    "html-webpack-plugin": "^5.0.0",
    "jsdom": "^22.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "trim": "^0.0.3",
    "webpack": "^5.0.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-virtual-modules": "^0.4.2"
  },
  "license": "MIT"
}
